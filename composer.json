{
  "name": "slick/di",
  "description": "Slick package for dependency injection container.",
  "keywords": [
    "slick",
    "dependency injection",
    "Di",
    "container"
  ],
  "minimum-stability": "dev",
  "homepage": "https://github.com/slickframework/di",
  "license": "MIT",
  "authors": [
    {
      "name": "Filipe Silva",
      "email": "silvam.filipe@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : ">=5.6.0",
    "container-interop/container-interop": "^1.1",
    "slick/common": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit" : "^4",
    "behat/behat": "^3",
    "phpspec/phpspec": "^3.2",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Slick\\Di\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slick\\Tests\\Di\\": "tests"
    }
  },
  "scripts": {
    "test": "phpspec run --format=dot; behat --format=progress",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.2-dev",
      "dev-develop": "2.3-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}