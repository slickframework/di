{
  "name": "slick/di",
  "description": "Slick package for dependency injection container.",
  "keywords": [
    "slick",
    "dependency injection",
    "Di",
    "container"
  ],
  "minimum-stability": "dev",
  "homepage": "https://github.com/slickframework/di",
  "license": "MIT",
  "version": "v2.8.0",
  "authors": [
    {
      "name": "Filipe Silva",
      "email": "silvam.filipe@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.2",
    "psr/container": "^1.0||^2.0",
    "slick/fswatch": "^0.1.0@dev"
  },
  "require-dev": {
    "behat/behat": "dev-master",
    "phpspec/phpspec": "^8.0@dev",
    "squizlabs/php_codesniffer": "4.0.x-dev"
  },
  "provide": {
    "psr/container-implementation": "1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Slick\\Di\\": "src"
    }
  },
  "scripts": {
    "test": "phpspec run --format=dot; behat --format=progress",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.8-dev",
      "dev-develop": "2.x-dev"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
